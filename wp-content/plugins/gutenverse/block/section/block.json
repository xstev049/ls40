    },
    "bottomDivider": {
      "type": "object",
      "default": {}
    },
    "margin": {
      "type": "object"
    },
    "padding": {
      "type": "object"
    },
    "zIndex": {
      "type": "object"
    },
    "animation": {
      "type": "object",
      "default": {}
